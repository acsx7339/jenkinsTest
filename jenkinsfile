pipeline {
    agent any
    stages {
        stage('---build---') {
            steps {
               sh echo 'build'
            }
        }
        stage('--test--') {
            steps {
              sh  echo 'test'
            }
        }
        stage('--finish--') {
            steps {
            sh   echo 'finishsaxd'
            }
        }
        stage('--finish2--') {
            steps {
               sh  echo 'test'
            }
        }
    }
}

