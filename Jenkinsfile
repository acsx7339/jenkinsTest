pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        sh 'echo \'hello\''
      }
    }
    stage('test') {
      steps {
        sh 'echo \'this is test'
      }
    }
    stage('depoly') {
      steps {
        sh 'echo \'qweqeqw\''
      }
    }
    stage('finish') {
      steps {
        sh 'echo \'finish\''
      }
    }
  }
}